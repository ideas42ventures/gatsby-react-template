    "test": "yarn jest",
    "test:watch": "yarn jest --watch",
    "test:e2e": "cypress open",
    "update-packages": "npx npm-check-updates -u --packageFile package.json && yarn install",
    "validate": "yarn lint && yarn type-check && yarn format && yarn test --passWithNoTests"
